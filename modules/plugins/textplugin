function initialize()
  print("This is the initialization of "..getName())
end

function getName()
  return "TestPlugin"
end

function printHelp()
  print("Helpidy help")
end

function execute(args)
  local catStr = ""
  
  for k, v in pairs(args) do
    catStr = catStr..v
  end
  
  print("You entered: "..catStr)
end
